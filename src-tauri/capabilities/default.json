{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions for image viewer",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:window:default",
    "core:window:allow-set-fullscreen",
    "core:window:allow-set-decorations",
    "fs:default",
    "fs:read-all",
    "fs:read-dirs",
    "fs:read-meta",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
